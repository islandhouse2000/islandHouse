{
  "version": 2,
  "builds": [
    {
      "src": "server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/socket",
      "dest": "server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_FRONTEND_URL": "https://${VERCEL_URL}",
    "NEXT_PUBLIC_SOCKET_URL": "wss://${VERCEL_URL}"
  }
}